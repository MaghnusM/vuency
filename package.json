{
  "name": "vuency",
  "description": "a vuejs concurrency management plugin",
  "version": "0.0.8",
  "author": "Alid Castano <alidcastano@gmail.com>",
  "module": "dist/build.js",
  "main": "dist/build.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot",
    "e2e": "cross-env NODE_ENV=testing node test/e2e/runner.js --env chrome --config test/e2e/nightwatch.conf.js",
    "unit": "cross-env NODE_ENV=testing karma start test/unit/karma.conf.js",
    "test": "npm run unit && npm run e2e",
    "purge": "rm -rf dist/",
    "bundle": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build": "npm run purge && npm run bundle",
    "upgrade": "npm --no-git-tag-version version patch && npm run build && npm publish"
  },
  "dependencies": {
    "vue": "^2.1.8"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.0.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-transform-vue-jsx": "^3.2.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-1": "^6.16.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chromedriver": "^2.26.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "http-server": "^0.9.0",
    "isparta-loader": "^2.0.0",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "mocha": "^3.2.0",
    "nightwatch": "^0.9.12",
    "selenium-server": "^3.0.1",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "vue-class-component": "^4.4.0",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.8",
    "webpack": "^2.2.0-rc.2",
    "webpack-dev-server": "^2.1.0-beta.9",
    "webpack-merge": "^1.1.2"
  }
}
